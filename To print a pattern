#include<stdio.h>
#inlcude<conio.h>
void main()
{
clrscr();//declaration of variables 
int i, j, k, l, m, n;
for(i=1; i<=5; i++)
{
a=97;
for(j=1; j<=6-i; j++)
{
printf(" %c",a);
a++;
}
printf("\n");
}
for(k=1; k<=5; k++)
{
a=122;
for(l=1; l<=k; l++)
{
printf(" %c",a);
if(l==5)
break;
a--;
}
printf("\n\);
}
for(m=1; m<=5; m++)
{
a=97;
for(n=1; n<=6-m; n++)
{
printf(" %c", a);
if(n==5)
break;
a++;
}
printf("\n");
}
getch();
}
