//to print first 100 prime number 
#include<stdio.h>
#include<conio.h>
#include<math.h>
void main()
{
clrscr();
int i, flag, count=0;//decleration of variable 
printf("first 100 prime number\n");
for(num=2; count<100; num++)
{
flag=0;
for(i=2; i<=sqrt(num); i++)
{
if(num%i==0)//condition for prime number 
{
flag=1;
break;
}
}
if(flag==1)
{
}
else
{
printf("%d", num);
count++;
}
}
getch();
}
